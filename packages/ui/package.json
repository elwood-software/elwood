{
  "name": "@elwood/ui",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**",
    "tailwind.config.js",
    "postcss.config.js"
  ],
  "scripts": {
    "build": "pnpm run build:ts && pnpm run build:css",
    "build:ts": "tsup src/index.ts --format esm,cjs --dts --external react",
    "build:css": "tailwindcss -i ./src/style.css -o dist/style.css",
    "clean": "rm -rf dist",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "jest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "jest": {
    "preset": "@elwood/jest-presets/jest/browser"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-form": "^0.1.0",
    "@radix-ui/react-hover-card": "^1.1.1",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-tooltip": "^1.1.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "lucide-react": "^0.408.0",
    "next-themes": "^0.3.0",
    "react-loading-skeleton": "^3.4.0",
    "react-textarea-autosize": "^8.5.3",
    "react-use": "^17.5.0",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.4.0",
    "tailwindcss": "^3.4.6",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@chromatic-com/storybook": "^1.6.1",
    "@elwood/eslint-config": "workspace:*",
    "@elwood/jest-presets": "workspace:*",
    "@elwood/typescript-config": "workspace:*",
    "@storybook/addon-essentials": "^8.2.4",
    "@storybook/addon-interactions": "^8.2.4",
    "@storybook/addon-links": "^8.2.4",
    "@storybook/addon-onboarding": "^8.2.4",
    "@storybook/blocks": "^8.2.4",
    "@storybook/react": "^8.2.4",
    "@storybook/react-vite": "^8.2.4",
    "@storybook/test": "^8.2.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.11",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.39",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^8.2.4",
    "storybook-dark-mode": "^4.0.2",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "tsup": "^8.1.2",
    "typescript": "^5.5.3"
  }
}
