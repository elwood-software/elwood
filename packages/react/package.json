{
  "name": "@elwood/react",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": ["dist/**"],
  "scripts": {
    "build": "pnpm build:js && pnpm build:css",
    "build:js": "tsup src/index.ts --format esm,cjs --dts --external react",
    "build:css": "tailwindcss -i ./src/style.css -o dist/style.css",
    "clean": "rm -rf dist",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "jest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "jest": {
    "preset": "@elwood/jest-presets/jest/browser"
  },
  "dependencies": {
    "@elwood/common": "workspace:*",
    "@elwood/js": "workspace:*",
    "@elwood/ui": "workspace:*",
    "@remix-run/router": "^1.16.1",
    "@tanstack/react-query": "^5.39.0",
    "@uppy/core": "^3.11.3",
    "@uppy/dashboard": "^3.8.3",
    "@uppy/react": "^3.3.1",
    "@uppy/tus": "^3.5.5",
    "clsx": "^2.1.1",
    "html-react-parser": "^5.1.10",
    "javascript-time-ago": "^2.5.10",
    "react-accessible-treeview": "^2.9.0",
    "react-dropzone": "^14.2.3",
    "react-pdf": "^8.0.2",
    "react-router-dom": "^6.23.1",
    "react-time-ago": "^7.3.3",
    "react-use": "^17.5.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.4.0",
    "@elwood/eslint-config": "workspace:*",
    "@elwood/jest-presets": "workspace:*",
    "@elwood/typescript-config": "workspace:*",
    "@storybook/addon-essentials": "^8.1.3",
    "@storybook/addon-interactions": "^8.1.3",
    "@storybook/addon-links": "^8.1.3",
    "@storybook/addon-onboarding": "^8.1.3",
    "@storybook/addon-themes": "^8.1.3",
    "@storybook/blocks": "^8.1.3",
    "@storybook/react": "^8.1.3",
    "@storybook/react-vite": "^8.1.3",
    "@storybook/test": "^8.1.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "jest": "^29.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^8.1.3",
    "tailwindcss": "^3.4.3",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-tsconfig-paths": "^4.3.2"
  }
}
